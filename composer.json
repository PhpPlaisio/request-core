{
  "name": "plaisio/request-core",
  "description": "PhpPlaisio: Core Request",
  "keywords": [
    "PhpPlaisio",
    "Request",
    "Core"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "ext-mbstring": "*",
    "plaisio/exception": "^1.3.0",
    "plaisio/request": "^1.0.1",
    "plaisio/trusted-host-authority": "^1.1.0"
  },
  "require-dev": {
    "phing/phing": "^3.0.1",
    "phpunit/phpunit": "^11.5.22",
    "plaisio/console-kernel": "^1.2.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Request\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Request\\Test\\": "test"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
