{
  "name": "plaisio/request-core",
  "description": "PhpPlaisio: Core Request",
  "keywords": [
    "PhpPlaisio",
    "Request",
    "Core"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "ext-mbstring": "*",
    "plaisio/exception": "^1.1",
    "plaisio/request": "^0.11.0"
  },
  "require-dev": {
    "phing/phing": "^2.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Request\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Request\\Test\\": "test"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
