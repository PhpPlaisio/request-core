{
  "name": "plaisio/request-core",
  "description": "PhpPlaisio: Core Request",
  "keywords": [
    "PhpPlaisio",
    "Request",
    "Core"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "ext-mbstring": "*",
    "plaisio/exception": "^1.3.0",
    "plaisio/helper-html": "^4.2.0",
    "plaisio/request": "^2.0.0",
    "plaisio/request-parameter-resolver": "^2.0.0",
    "plaisio/trusted-host-authority": "^1.1.0",
    "setbased/helper-cast": "^3.1.0"
  },
  "require-dev": {
    "phing/phing": "^3.0.1",
    "phpunit/phpunit": "^11.5.25",
    "plaisio/console-kernel": "^1.2.0"
  },
  "conflict": {
    "plaisio/cgi-core": "<1.10.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Request\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Request\\Test\\": "test"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
